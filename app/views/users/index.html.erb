<h1>Users list</h1>
<hr />
<table border=1>
  <tr>
    <td>
      Username
    </td>
    <td>
      Email
    </td>
    <td>
      Created At
    </td>
  </tr>

<% @users.each do |user| %>
<tr>
  <td>
    <%= user.username %>
  </td>
  <td>
    <%= user.email %>
  </td>
  <td>
    <%= user.created_at %>
  </td>
</tr>
<% end %>
</table>
